<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Transcribe_controller">
   <center>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button layoutX="193.0" layoutY="161.0" mnemonicParsing="false" onAction="#transcribe" text="Transcribe!" />
            <TextField fx:id="endTime" layoutX="245.0" layoutY="117.0" />
            <TextField fx:id="beginTime" layoutX="53.0" layoutY="117.0" />
            <Label layoutX="62.0" layoutY="89.0" text="Transcribe begin time" />
            <Label layoutX="267.0" layoutY="89.0" text="Transcribe end time" />
            <Button layoutX="14.0" layoutY="362.0" mnemonicParsing="false" onAction="#quitButton" text="Quit" />
            <Button layoutX="272.0" layoutY="362.0" mnemonicParsing="false" onAction="#logOutButton" text="Log out" />
            <Button layoutX="467.0" layoutY="263.0" mnemonicParsing="false" onAction="#previewButton" text="Preview" />
            <Button layoutX="423.0" layoutY="55.0" mnemonicParsing="false" onAction="#uploadFile" text="Choose a file!" />
            <Label fx:id="fileLabel" layoutX="306.0" layoutY="165.0" />
         </children>
      </AnchorPane>
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
         <effect>
            <Blend />
         </effect>
      </MenuBar>
   </top>
</BorderPane>
